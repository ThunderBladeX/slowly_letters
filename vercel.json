{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/favicon\\.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/favicon\\.png",
            "dest": "/favicon.png"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
